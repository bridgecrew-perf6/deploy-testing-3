{
  "name": "deploy-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "next dev -p 12300",
    "build": "next build",
    "build-standalone": "next build && next export",
    "start": "next start",
    "test": "jest",
    "test:config": "jest src/__tests__/config/ --testPathIgnorePatterns",
    "integration-test": "start-server-and-test start http://localhost:12300 'cypress run'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:check:staged": "pretty-quick --staged --check",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "updateLPsAPR": "NODE_PATH=./src ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/updateLPsAPR.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "ajv": "^8.10.0",
    "bignumber.js": "^9.0.2",
    "canvas-confetti": "^1.5.1",
    "cids": "^1.1.9",
    "date-fns": "^2.28.0",
    "easymde": "^2.16.1",
    "fast-json-stable-stringify": "^2.1.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "js-cookie": "^3.0.1",
    "lightweight-charts": "^3.7.0",
    "lodash": "^4.17.21",
    "multicodec": "^3.2.1",
    "multihashes": "^4.0.3",
    "next": "^12.0.10",
    "numeral": "^2.0.6",
    "paybolt-uikit": "https://github.com/paybolt/paybolt-uikit",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-countup": "^6.1.1",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.0",
    "react-redux": "^7.2.6",
    "react-transition-group": "^4.4.2",
    "react-window": "^1.8.6",
    "recharts": "^2.1.9",
    "redux-persist": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "split-grid": "^1.0.11",
    "styled-components": "^5.3.3",
    "swiper": "^8.0.6",
    "swr": "^1.2.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10"
  }
}
